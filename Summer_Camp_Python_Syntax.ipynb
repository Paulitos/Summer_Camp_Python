      "cell_type": "code",
      "source": [
        "# Ejemplo For Loop\n",
        "my_list = [\"pie\",\"cake\",\"cookie\"]\n",
        "for item in my_list:\n",
        "  print(item)\n",
        "\n",
        "# Ejemplo While Loop\n",
        "x = 0\n",
        "while (x < 3):\n",
        "  print(x)\n",
        "  x = x + 1\n",
        "\n",
        "# Ejemplo If-Statement \n",
        "x = 5\n",
        "if x != 3:\n",
